// api_spec.json
{
  "openapi": "3.0.1",
  "info": {
    "title": "Interdimensional Communication API",
    "version": "1.0.0",
    "description": "INTERDIMENSIONAL COMMUNICATION API!!!!!!!!!"
  },
  "paths": {
