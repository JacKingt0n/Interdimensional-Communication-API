              "schema": {
                "$ref": "#/components/schemas/NeutrinoGravitationalWaveMessage"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Message sent successfully"
          }
        }
      }
    },
    // ... other endpoints
  },
  "components": {
    "schemas": {
      "NeutrinoGravitationalWaveMessage": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": ["neutrino", "gravitational_wave"]
          },
          "data": {
            "type": "string",
            "format": "base64"
          },
          // ... other properties
        }
      }
    }
  }
}
